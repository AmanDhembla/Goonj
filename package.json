{
  "name": "goonj",
  "version": "1.0.0",
  "description": "Web app to bring all the ngo's to a common platform",
  "main": "index.js",
  "engines": {
    "node": "8.6.0",
    "npm": "5.4.2"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AmanDhembla/Goonj.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AmanDhembla/Goonj/issues"
  },
  "homepage": "https://github.com/AmanDhembla/Goonj#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.3",
    "mongoose": "^5.7.5",
    "nodemon": "^1.17.3",
    "normalize.css": "^8.0.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0"
  }
}
